{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "CloudWatchLogsAccess",
      "Effect": "Allow",
      "Action": [
        "logs:CreateLogGroup",
        "logs:CreateLogStream",
        "logs:PutLogEvents",
        "logs:DescribeLogStreams",
        "logs:DescribeLogGroups"
      ],
      "Resource": [
        "arn:aws:logs:eu-north-1:535002890586:log-group:/aws/ec2/docohpp*",
        "arn:aws:logs:eu-north-1:535002890586:log-group:/application/docohpp*"
      ]
    }
  ]
}

