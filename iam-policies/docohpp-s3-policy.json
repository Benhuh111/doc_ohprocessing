{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "S3DocumentBucketAccess",
      "Effect": "Allow",
      "Action": [
        "s3:PutObject",
        "s3:GetObject",
        "s3:DeleteObject"
      ],
      "Resource": "arn:aws:s3:::docohpp-documents-behu-20250827-001/*"
    },
    {
      "Sid": "S3BucketList",
      "Effect": "Allow",
      "Action": "s3:ListBucket",
      "Resource": "arn:aws:s3:::docohpp-documents-behu-20250827-001"
    }
  ]
}

